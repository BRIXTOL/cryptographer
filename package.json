			"test/**/*"
		],
		"concurrency": 5
	},
	"devDependencies": {
		"@brixtol/eslint-config-javascript": "workspace:^2.0.0",
		"@brixtol/prettier-config": "workspace:^1.0.3",
		"@rollup/plugin-commonjs": "^17.1.0",
		"ava": "^3.15.0",
		"chalk": "^4.1.0",
		"esm": "^3.2.25",
		"rollup-plugin-terser": "^7.0.2"
	},
	"peerDependencies": {
		"ava": "^3.13.0",
		"esm": "^3.2.25",
		"rollup": "^2.26.11"
	}
