#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/Panoply/Sites/brixtol/serverless/node_modules/.pnpm/ava@3.13.0/node_modules/ava/cli.js/node_modules:/Users/Panoply/Sites/brixtol/serverless/node_modules/.pnpm/ava@3.13.0/node_modules/ava/node_modules:/Users/Panoply/Sites/brixtol/serverless/node_modules/.pnpm/ava@3.13.0/node_modules:/Users/Panoply/Sites/brixtol/serverless/node_modules/.pnpm/node_modules:/Users/Panoply/Sites/brixtol/serverless/node_modules:/Users/Panoply/Sites/brixtol/node_modules:/Users/Panoply/Sites/node_modules:/Users/Panoply/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ava@3.13.0/node_modules/ava/cli.js" "$@"
else 
  exec node  "$basedir/../../../../node_modules/.pnpm/ava@3.13.0/node_modules/ava/cli.js" "$@"
fi
